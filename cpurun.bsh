#!/bin/bash

#Magical Amazing Time Saving Wonder Script
# TLDR Automated simulation runner

ALGOS="MIPB OSB ISB"


for net in $*; do
  for algo in $ALGOS; do
    net=`basename $net`
    scenario="${algo}_${net}"
    ./multiuserdsm.py -n networks/$net -a $algo -S ${scenario}_CPU -l ${scenario}_CPU.log
  done
done


